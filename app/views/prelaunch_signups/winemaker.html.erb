<div id="wrap">
  <div id="main">
    <div id="header">
      <h1>socialwino</h1>
    </div><!-- End Header -->
    <div class="social">
              <ul>
                  <li><a class="facebook" href="http://www.facebook.com/socialwino">Facebook</a></li>
                  <li><a class="twit" href="http://twitter.com/socialwino">Twitter</a></li>
              </ul>                         
    </div><!-- End Social -->
    <div class="clear"></div>
       <div id="container">
              <div class="subscribe">
                         <h3>Join Us</h3>
                         <%= form_for(@prelaunch_signup, :html => { :id => "form-email" }) do |f| %>
                         <% if @prelaunch_signup.errors.any? %>
                         <div class="prelaunch-errors">
                         <div class="ui-widget">
                           <div class="ui-state-error ui-corner-all" style="padding: 0 .7em;">
                             <% @prelaunch_signup.errors.full_messages.each do |msg| %>
                               <span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
                               <%= msg %>
                             <% end %>
                           </div>
                         </div>
                         </div>
                         <% end %>

                         <p>
                         <%= f.text_field :email,:placeholder => "enter your e-mail address" %>
                         <%= f.submit "" %>
                         </p>

                         <% end %>
               </div><!-- End Subscribe -->
       </div><!-- End Container -->            
  </div><!-- End Main -->  
  
  <div class="clear"></div>
  <div id="footer">
         <div class="footerbg">
           
                <div class="clear"></div>
                <div class="copy">
                  <p>Socialwino is brought to you by the fine folks at <a href="http://suntouchersoftware.com">Suntoucher LLC</a>. Â© 2010 Suntoucher LLC. All rights reserved.</p>
                </div><!-- End Copy -->
        </div><!-- End Footerbg -->       
  </div><!-- End Footer -->
</div><!-- End Wrap -->